{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "predeploy": "npm run generate",
    "deploy": "gh-pages -d .output/public -b gh-pages",
    "deploy:clean": "gh-pages -d .output/public -b gh-pages --remove '**/*'",
    "deploy:github-actions": "npm run generate",
    "vercel-build": "nuxt build",
    "clean": "rm -rf .nuxt .output node_modules package-lock.json",
    "fresh-install": "npm run clean && npm install",
    "verify-deps": "npm list @nuxt/content @nuxt/icon @nuxtjs/seo @nuxtjs/sitemap"
  },
  "dependencies": {
    "@nuxt/content": "^3.6.3",
    "@nuxt/icon": "^1.15.0",
    "@nuxt/image": "^1.11.0",
    "@nuxtjs/seo": "^3.1.0",
    "@nuxtjs/sitemap": "^7.4.3",
    "@supabase/supabase-js": "^2.52.0",
    "better-sqlite3": "^12.2.0",
    "nuxt": "^4.0.2",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/logos": "^1.2.5",
    "@iconify-json/mdi": "^1.2.3",
    "@nuxt/eslint": "^1.6.0",
    "@vite-pwa/nuxt": "^1.0.4",
    "cssnano": "^7.1.0",
    "eslint": "^9.31.0",
    "gh-pages": "^6.3.0",
    "markdown-it": "^14.1.0",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
